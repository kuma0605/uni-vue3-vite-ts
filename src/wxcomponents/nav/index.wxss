/* components/nav/nav.wxss */
.nav_wrapper{
    width:750rpx;
}
.nav-outer{
    z-index:999;
    position:fixed;
    width:750rpx;
}
.nav-ct{
    display:flex;
    align-items:center;
    justify-content:center;
    box-sizing:border-box;
    position:relative;
}

.arrow_outer {
    position: absolute;
    left:18rpx;
    top:50%;
    transform: translateY(-50%);

    display: flex;
    align-items: center;

}

.arrow_left {
    width: 36rpx;
    height: 36rpx;
}

